    "action",
    "images"
  ],
  "license": "ISC",
  "scripts": {
    "test": "nodemon node ./test/main.test.js"
  },
  "engines": {
    "node": ">=20.12.2"
  },
  "dependencies": {
    "canvas": "^3.1.0",
    "hex-color-regex": "^1.1.0",
    "neekuro": "file:",
    "nekoapi.beta": "^1.0.5",
    "node-fetch": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CacaoNk0027/neekuro_package.git"
  },
  "bugs": {
